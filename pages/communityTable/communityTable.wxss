/* pages/communityTable/communityTable.wxss */
page{
  background-color: white;
}

.title{
  width: 100%;
  text-align: center;
  color: #2664b6;
  height: 100rpx;
  line-height: 100rpx;
  font-weight: 700;
  font-size: 50rpx;
}

.item{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 30rpx;
  border-bottom: 1rpx solid #dddddd;
}

.name{
  width:50%;
  color: #2664b6;
  padding-left: 30rpx;
  padding-bottom: 20rpx;
  font-size: 40rpx;
}

.kinds{
  padding-bottom: 30rpx;
  color: #555555;
  font-size: 40rpx;
}

.xiaoqu{
  padding-bottom: 30rpx;
  padding-right: 10rpx;
  color: #555555;
  font-size: 40rpx;
}